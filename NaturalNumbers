//wap to written the sum on n natural numbers 
public class Natural{

    public static void main(String[] args){

        Scanner obj = new Scanner(System.in);
        int num = obj.nextInt();
        int sum = 0;

        for(int i=1; i >= num; i++){
            sum = sum + i;
        }
        System.out.println(sum);

    }
}